{
  "name": "zasper",
  "author": "Prasun Anand <prasun@zasper.io>",
  "description": "IDE",
  "version": "0.1.0-alpha",
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "@codemirror/commands": "^6.6.2",
    "@codemirror/lang-go": "^6.0.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/lang-less": "^6.0.2",
    "@codemirror/lang-markdown": "^6.2.5",
    "@codemirror/lang-python": "6.0.0",
    "@codemirror/lang-sass": "^6.0.2",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/lint": "^6.8.4",
    "@codemirror/view": "^6.33.0",
    "@uiw/codemirror-theme-darcula": "^4.23.6",
    "@uiw/codemirror-theme-github": "^4.23.6",
    "@uiw/react-codemirror": "4.22.2",
    "@xterm/addon-attach": "0.11.0",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "0.11.0",
    "@xterm/xterm": "5.5.0",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.2.0",
    "deepdash": "4.5.1",
    "dompurify": "^3.2.4",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.2.0",
    "electron-store": "^10.0.0",
    "html-react-parser": "^3.0.1",
    "html-to-react": "^1.5.0",
    "jotai": "1.7.2",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.33.0",
    "react": "18.3.1",
    "react-bootstrap": "^2.4.0",
    "react-codemirror-merge": "^4.23.2",
    "react-cookie": "4.1.1",
    "react-dom": "18.3.1",
    "react-icons-kit": "^1.3.1",
    "react-images-uploading": "3.1.7",
    "react-login-github": "1.0.8",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^2.1.2",
    "react-router-dom": "^6.30.0",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.6",
    "react-terminal": "1.3.0",
    "react-toastify": "10.0.5",
    "rehype-raw": "^7.0.0",
    "typescript": "^4.9.5",
    "uuid": "11.0.5",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.2",
    "websocket": "1.0.34"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/dompurify": "^2.4.0",
    "electron": "^30.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^27.5.1",
    "prettier": "^3.5.3",
    "sass": "^1.77.8",
    "ts-standard": "^12.0.2"
  },
  "scripts": {
    "lint": "eslint --fix src",
    "format": "prettier --write .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "electron-builder --mac --linux",
    "electron-package-mac": "electron-builder --mac",
    "electron-package-linux": "electron-builder --linux",
    "electron-package-rpm": "electron-builder --linux rpm",
    "electron-package-snap": "electron-builder --linux snap",
    "electron-package-appImage": "electron-builder --linux AppImage"
  },
  "build": {
    "appId": "io.zasper.app",
    "afterSign": "notarize.js",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "icon": "build/favicon.ico"
    },
    "mac": {
      "icon": "build/logo512.png",
      "target": "dmg"
    },
    "linux": {
      "icon": "build/logo512.png",
      "target": "deb"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
